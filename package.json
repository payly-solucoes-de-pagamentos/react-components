{
  "name": "payly-components",
  "version": "0.2.1-beta",
  "description": "",
  "author": "",
  "license": "MIT",
  "repository": "/payly-components",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "publishConfig": {
    "registry": "https://repo.devops.processing-cloud.internal.payly.com.br/repository/npm-releases/"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --colors --coverage",
    "storybook": "start-storybook",
    "build": "rollup -c",
    "show-config": "rollup --config",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.1.4",
    "@svgr/rollup": "^4.3.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-minify": "^0.5.0",
    "cross-env": "^5.1.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^1.2.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "prettier": "^1.17.1",
    "prop-types": "^15.5.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.13.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-ignore": "^1.0.5",
    "rollup-plugin-multi-input": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-url": "^2.2.1",
    "sinon": "^7.3.2",
    "styled-components": "^4.2.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme",
      "<rootDir>src/setupTests.js"
    ],
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "statements": -10
      }
    },
    "collectCoverageFrom": [
      "src/components/**/*.{js, jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "styles.js"
    ],
    "bail": 1
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "moment": "^2.24.0",
    "react-dates": "^20.2.3",
    "react-moment-proptypes": "^1.6.0",
    "react-select": "^3.0.4"
  }
}
